    "@types/chrome": "^0.0.320",
    "@types/html-to-text": "^9.0.4",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "fast-xml-parser": "^5.2.3",
    "fuse.js": "^7.1.0",
    "html-to-text": "^9.0.5",
    "html-webpack-plugin": "^5.6.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "style-loader": "^4.0.0",
